public class Pessoa {
	
	private String nome; 
	private int idade; 
	private float altura; 
	private float peso; 
	private char sexo; 

	
	public void setNome(String nome) 
	{ 
	this.nome = nome; 
	} 

	public String getNome() 
	{ 
	return this.nome; 
	} 

	public void setIdade(int idade) 
	{ 
	this.idade = idade; 
	} 

	public int getIdade() 
	{ 
	return this.idade; 
	} 

	public void setAltura(float altura) 
	{ 
	this.altura = altura; 
	} 

	public float getAltura() 
	{ 
	return this.altura; 
	} 

	public void setPeso(float peso) 
	{ 
	this.peso = peso; 
	} 

	public float getPeso() 
	{ 
	return this.peso; 
	} 

	public void setSexo(char sexo) 
	{ 
	this.sexo = sexo; 
	} 

	public char getSexo() 
	{ 
	return this.sexo; 
	} 


}
